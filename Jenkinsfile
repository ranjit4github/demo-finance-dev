pipeline{
    // Where your pipeline to be executed?
    agent any

    // Different Stages
    stages{
        stage ('Check Out'){
          steps{
            echo "Checkout.."
          }
        }
        stage ('Build'){
            steps{
                echo "Building Successful!"
            }
        }
        stage ('Test'){
            steps{
                echo "Testing Successful!"
            }
        }
    }
}
